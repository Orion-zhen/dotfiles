{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "audio_config": {
        "audio": "pipewire"
    },
    "bootloader": "Systemd-boot",
    "config_version": "2.8.6",
    "debug": false,
    "hostname": "archlinux",
    "kernels": [
        "linux"
    ],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [
            {
                "name": "archlinuxcn",
                "sign_check": "Optional",
                "sign_option": "TrustAll",
                "url": "https://mirrors.aliyun.com/archlinuxcn/$arch"
            },
            {
                "name": "arch4deu",
                "sign_check": "Optional",
                "sign_option": "TrustAll",
                "url": "https://mirrors.aliyun.com/arch4edu/$arch"
            },
            {
                "name": "chaotic-aur",
                "sign_check": "Optional",
                "sign_option": "TrustAll",
                "url": "https://mirrors.aliyun.com/chaotic-aur/$arch"
            }
        ],
        "mirror_regions": {}
    },
    "network_config": {
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "packages": [
        "docker",
        "docker-compose",
        "docker-buildx",
        "openssh",
        "openbsd-netcat",
